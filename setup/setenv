setenv_file_generate() {
    local file=$1

    cat > ${file} <<EOF
if [[ "\${setenv_BASE_PATH}" == "" ]]; then
    export setenv_BASE_PATH=\$PATH
fi
PATH=\${setenv_BASE_PATH}
PATH=\$PATH:\$HOME/.cache/rebar3/bin
RUNROOT=\$(mktemp -d /tmp/${APP}.XXXXXX)
ERLROOT=\$(kerl list installations | grep ${ERLANG_VERSION} |head -n 1| cut -f 2 -d' ')
source \${ERLROOT}/activate
export PATH SRCROOT BLDROOT RUNROOT
EOF
}
